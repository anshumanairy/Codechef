    "def valid():\n",
    "    T=int(input(\"\"))\n",
    "    list1=[\"\"]*T\n",
    "    for i in range(0,T):\n",
    "        c=input(\"\")\n",
    "        A,B,C=map(int,c.split())\n",
    "        if(A+B+C==180):\n",
    "            list1[i]=\"YES\"\n",
    "        else:\n",
    "            list1[i]=\"NO\"\n",
    "    print(*list1,sep=\"\\n\")\n",
    "valid()"
