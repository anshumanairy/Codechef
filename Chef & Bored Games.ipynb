    "def chess():\n",
    "    T=int(input(\"\"))\n",
    "    list1=[\"\"]*T\n",
    "    for i in range(0,T):\n",
    "        N=int(input(\"\"))\n",
    "        j=N\n",
    "        s=0\n",
    "        while j>0:\n",
    "            s=s+(j*j)\n",
    "            j=j-2\n",
    "        list1[i]=s\n",
    "    print(*list1,sep='\\n')\n",
    "chess()"
